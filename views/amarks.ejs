<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/x-icon" href="favicon.ico">
    <link rel="stylesheet" href="/css/admin.css">
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css">
    <link rel="stylesheet" href="https://kit.fontawesome.com/90644867f2.css" crossorigin="anonymous">
    <title>Admin</title> 
</head>
<body>
    <nav>
        <div class="logo-name">
            <span class="logo_name">Tech Fanatics</span>
        </div>
        <div class="menu-items">
            <div class="logop">
                <div class="logo" style="background-image:url(../images/logo.png)"></div>
                     <ul class="nav-links">
                     <li><a href="/admin">
                             <i class="uil uil-estate"></i>
                             <span class="link-name">Home</span>
                         </a></li>
                         <li><a href="/ausers">
                            <i class="uil uil-files-landscapes"></i>
                            <span class="link-name">Trainees Details</span>
                        </a></li>
                         <li><a href="/amarks">
                             <i class="uil uil-files-landscapes"></i>
                             <span class="link-name">Mark Details</span>
                         </a></li>
                         <li><a href="/aupdate">
                             <i class="uil uil-comments"></i>
                             <span class="link-name">Modify Users</span>
                         </a></li>
                     </ul>
                     <ul class="logout-mode">
                        <li><a href="/alogin">
                            <i class="uil uil-signout"></i>
                            <span class="link-name">Logout</span>
                        </a></li>
                        <li class="mode">
                           <a href="#">
                              <i class="uil uil-moon"></i>
                              <span class="link-name">Dark Mode</span>
                           </a>
                           <div class="mode-toggle">
                                <span class="switch"></span>
                           </div>
                        </li>
                    </ul>
        </div><!--menu-items-->
    </nav>

    <section class="dashboard">
             <div class="top">
                  <i class="uil uil-bars sidebar-toggle"></i>
                  <div class="title">
                           <span class="text">Admin</span>
                      </div>
                 <!--search-box-->
             </div><!--top-->
             
             <div class="dash-content">
                <div class="overview">
                    <div class="title">
                         <i class="uil uil-files-landscapes"></i>
                         <span class="text">Courses</span>
                    </div><!--title-->
                    <div class="boxes">
                         <button class="box box2" >
                                 <i class="uil uil-user-md"></i>
                                 <span class="text">No of Courses
                                     <h4>10</h4>  
                                 </span>
                         </button>
                         
                      </div><!--boxes-->
                  </div><!--overview-->
              <div class="activity">

                    <form method="post">
                         <div class="title">
                              <i class="uil uil-upload"></i>
                              <span class="text">Mark Details</span>
                         </div><!--title-->
                    <div class="activity-data">
    
                    <table>
                    <thead >
                        
                        <tr>                
                                           <th>Quiz Id</th>
                                           <th>User Name</th>
                                           <th>Score</th>

                       </tr>
                       
                </thead>
                
                <tbody>
                    <% data.forEach(quizResult => { %>
                      <tr>
                        <td><%= quizResult._id ? quizResult._id : 'N/A' %></td>
                        <td><%= quizResult.user ? quizResult.user.username : 'N/A' %></td>
                        <td><%= quizResult.score ? quizResult.score : 'N/A' %></td>
                      </tr>
                    <% }); %>
                  </tbody>
                  

                  
                  
                    </table>
                    </div>
                     

                    </form>
               </div>
      
       
                
        </div>
    </section>
    
    <script src="/scripts/admin.js"></script>
</body>
</html>